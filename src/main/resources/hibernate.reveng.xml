<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="100" not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="char" length="1"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="LONGVARCHAR" hibernate-type="string"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="19" not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-name="permissao" match-catalog="pvp_db"></table-filter>
	<table-filter match-name="permissao_usuario"
		match-catalog="pvp_db">
	</table-filter>
	<table-filter match-name="tipo_usuario" match-catalog="pvp_db"></table-filter>
	<table catalog="pvp_db" name="tipo_usuario">
		<column name="codigo"></column>
		<column name="nome"></column>
		<column name="descricao"></column>
	</table>
	<table catalog="pvp_db" name="permissao_usuario">
		<column name="cod_tipo_usuario"></column>
		<column name="cod_permissao"></column>
	</table>
	<table catalog="pvp_db" name="permissao">
		<column name="codigo"></column>
		<column name="nome"></column>
		<column name="role"></column>
	</table>
</hibernate-reverse-engineering>